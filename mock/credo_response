<script>
    function onlineBankDropDownButtonMobileFunction() {
        document.getElementById("mobileDropDown").classList.toggle("mobileShow");
        document.querySelector("#mobileArrow").classList.toggle("arrowAnimation");
    }

    function onlineBankDropDownButtonFunction() {
        document.getElementById("dropdownElements").classList.toggle("show");
        document.getElementById("desktopArrow").classList.toggle("arrowAnimation");
    }
</script>
